[MASTER]
ignore=*.txt, venv, .git, db, *.sh, *.error, *.log, *.yaml, *.md

[MESSAGES CONTROL]
disable=missing-docstring, no-member, line-too-long, no-self-use, bad-continuation, wildcard-import, broad-except, invalid-name, wrong-import-order, multiple-statements, logging-fstring-interpolation, arguments-differ, unused-argument, unused-wildcard-import

[DESIGN]
min-public-methods=0
max-public-methods=50
